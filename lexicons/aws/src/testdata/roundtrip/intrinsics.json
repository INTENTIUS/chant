{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Template with intrinsic functions",
  "Resources": {
    "MyBucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": { "Fn::Sub": "${AWS::StackName}-data" }
      }
    },
    "MyQueue": {
      "Type": "AWS::SQS::Queue",
      "Properties": {
        "QueueName": { "Fn::Join": ["-", ["my", "queue", { "Ref": "AWS::Region" }]] }
      }
    }
  }
}
